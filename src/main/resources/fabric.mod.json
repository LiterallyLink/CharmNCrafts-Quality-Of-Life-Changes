{
	"schemaVersion": 1,
	"id": "charmncraft",
	"version": "${version}",
	"name": "CharmNCraft",
	"description": "CharmNCraft adds magical XP vials for storing experience points, along with various creatures and items to enhance your Minecraft experience.",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/charmncraft/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.charmed.charmncraft.CharmNCraft"
		],
		"client": [
			"com.charmed.charmncraft.CharmNCraftClient"
		]
	},
	"mixins": [
		"charmncraft.mixins.json",
		{
			"config": "charmncraft.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}